{
  "app": {
    "number_of_threads": 1,
    "log": { "log_level": "DEBUG" },
    "filters": [
      {
        "name": "CorsFilter",
        "paths": ["/"],
        "excluded_paths": [],
        "config": {
          "allowed_headers": "Content-Type, Authorization, X-Requested-With",
          "allowed_methods": "GET, POST, PUT, DELETE, OPTIONS",
          "allow_credentials": false,
          "max_age": 86400,
          "expose_headers": "Content-Length, X-Custom-Header",
          "allow_any_origin": true
        }
      }
    ]
  },
  "listeners": [
    {
      "address": "0.0.0.0",
      "port": 8082,
      "https": false
    }
  ],
  "db_clients": [
    {
      "name": "default",
      "rdbms": "postgresql",
      "host": "host.docker.internal",
      "port": 5432,
      "dbname": "NetworkCoverageOptimization",
      "user": "postgres",
      "passwd": "postgres",
      "is_fast": false
    }
  ]
}
